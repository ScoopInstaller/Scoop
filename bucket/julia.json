    "installer": {
        "file": "julia-installer.exe",
        "args": [
            "/S",
            "/D=$dir"
        ]
    },
    "uninstaller": {
        "file": "uninstall.exe",
        "args": "/S"
    },
    "env_set": {
        "JULIA_HOME": "$dir\\bin",
