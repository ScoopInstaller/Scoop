{
    "homepage": "https://bazel.build",
    "version": "0.14.1",
    "license": "Apache-2.0",
    "bin": "bazel.exe",
    "suggest": {
        "MSYS2": [
            "msys2"
        ],
        "Python27": [
            "versions/python27"
        ],
        "curl": [
            "curl"
        ]
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/bazelbuild/bazel/releases/download/0.14.1/bazel-0.14.1-windows-x86_64.zip",
            "hash": "caf32aa7be4d8962093a88059ac34501816f9c8bcfbeef16a98de7e038aa86ec"
        }
    },
    "checkver": {
        "github": "https://github.com/bazelbuild/bazel"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/bazelbuild/bazel/releases/download/$version/bazel-$version-windows-x86_64.zip"
            }
        },
        "hash": {
            "mode": "extract",
            "url": "$url.sha256"
        }
    },
    "env_set": {
        "BAZEL_SH": "$(appdir msys2)\\current\\usr\\bin\\bash.exe"
    }
}
