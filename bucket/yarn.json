{
    "homepage": "https://yarnpkg.com/",
    "license": "BSD",
    "version": "1.4.0",
    "suggest": {
        "Node.js": [
            "nodejs",
            "nodejs-lts"
        ],
        "NVM": [
            "nvm"
        ]
    },
    "url": "https://yarnpkg.com/downloads/1.4.0/yarn-1.4.0.msi",
    "hash": "d9063b187fc7ca47493f696a470bbebf960982ea4774e4e4ff0362777e6d4f42",
    "persist": [
        "cache",
        "bin"
    ],
    "post_install": "
        yarn config set cache-folder \"$dir\\cache\"
        yarn config set prefix \"$dir\"
    ",
    "env_add_path": [
        "bin",
        "Yarn\\bin"
    ],
    "checkver": {
        "url": "https://yarnpkg.com/latest-version",
        "re": "([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://yarnpkg.com/downloads/$version/yarn-$version.msi"
    }
}
