{
    "version": "1.12",
    "homepage": "https://golang.org",
    "license": "BSD-3-Clause",
    "extract_dir": "go",
    "env_set": {
        "GOROOT": "$dir"
    },
    "bin": [
        "bin/go.exe",
        "bin/gofmt.exe",
        "bin/godoc.exe"
    ],
    "installer": {
        "script": "add_first_in_path \"$env:USERPROFILE\\go\\bin\" $global"
    },
    "uninstaller": {
        "script": "remove_from_path \"$env:USERPROFILE\\go\\bin\" $global"
    },
    "architecture": {
        "64bit": {
            "url": "https://storage.googleapis.com/golang/go1.12.windows-amd64.zip",
            "hash": "880ced1aecef08b3471a84381b6c7e2c0e846b81dd97ecb629b534d941f282bd"
        },
        "32bit": {
            "url": "https://storage.googleapis.com/golang/go1.12.windows-386.zip",
            "hash": "c6606bfdc4d8b080fc40f72a072eb380ead77a02a4f99a6b953df6d9c7029970"
        }
    },
    "checkver": "Build version go([\\d\\.]+)\\.",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://storage.googleapis.com/golang/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://storage.googleapis.com/golang/go$version.windows-386.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    },
    "notes": [
        "Your GOROOT has been set to: $dir",
        "You can run 'go env GOROOT' to view this at any time.",
        "\"$env:USERPROFILE\\go\\bin)\" has been added to your PATH."
    ]
}
