{
    "version": "1.12.3",
    "homepage": "https://golang.org",
    "license": "BSD-3-Clause",
    "extract_dir": "go",
    "env_set": {
        "GOROOT": "$dir"
    },
    "bin": [
        "bin/go.exe",
        "bin/gofmt.exe",
        "bin/godoc.exe"
    ],
    "installer": {
        "script": "add_first_in_path \"$env:USERPROFILE\\go\\bin\" $global"
    },
    "uninstaller": {
        "script": "remove_from_path \"$env:USERPROFILE\\go\\bin\" $global"
    },
    "architecture": {
        "64bit": {
            "url": "https://storage.googleapis.com/golang/go1.12.3.windows-amd64.zip",
            "hash": "1806e089e85b84f192d782a7f70f90a32e0eccfd181405857e612f806ec04059"
        },
        "32bit": {
            "url": "https://storage.googleapis.com/golang/go1.12.3.windows-386.zip",
            "hash": "de3446df6f764030c5945800134c191f092dc14d2301f12fee6c8f611ac18829"
        }
    },
    "checkver": "Build version go([\\d\\.]+)\\.",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://storage.googleapis.com/golang/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://storage.googleapis.com/golang/go$version.windows-386.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    },
    "notes": [
        "Your GOROOT has been set to: $dir",
        "You can run 'go env GOROOT' to view this at any time.",
        "\"$env:USERPROFILE\\go\\bin)\" has been added to your PATH."
    ]
}
