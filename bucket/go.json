{
    "version": "1.11.1",
    "homepage": "https://golang.org",
    "license": "BSD-3-Clause",
    "extract_dir": "go",
    "env_set": {
        "GOROOT": "$dir"
    },
    "bin": [
        "bin/go.exe",
        "bin/gofmt.exe",
        "bin/godoc.exe"
    ],
    "installer": {
        "script": "add_first_in_path \"$env:USERPROFILE\\go\\bin\" $global"
    },
    "uninstaller": {
        "script": "remove_from_path \"$env:USERPROFILE\\go\\bin\" $global"
    },
    "architecture": {
        "64bit": {
            "url": "https://storage.googleapis.com/golang/go1.11.1.windows-amd64.zip",
            "hash": "230a08d4260ded9d769f072512a49bffe8bfaff8323e839c2db7cf7c9c788130"
        },
        "32bit": {
            "url": "https://storage.googleapis.com/golang/go1.11.1.windows-386.zip",
            "hash": "5cc3681c954e23d40b0c2565765ec34f4b4e834348e03e1d1e6fd1c3a75b8202"
        }
    },
    "checkver": "Build version go([\\d\\.]+)\\.",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://storage.googleapis.com/golang/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://storage.googleapis.com/golang/go$version.windows-386.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    },
    "notes": [
        "Your GOROOT has been set to: $dir",
        "You can run 'go env GOROOT' to view this at any time.",
        "\"$env:USERPROFILE\\go\\bin)\" has been added to your PATH."
    ]
}
